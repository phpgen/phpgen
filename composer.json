{
    "name": "phpgen/phpgen",
    "description": "PHP source code generator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PHPGen\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "localusercamp",
            "email": "user062199@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "laravel/pint": "dev-main",
        "pestphp/pest": "2.x-dev",
        "symfony/var-dumper": "7.1.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
